<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="user" class="com.legend.scls.User">
        <property name="id" value="111"></property>
        <property name="name" value="robin"></property>
        <property name="age" value="26"></property>
    </bean>

    <bean id="user1" class="com.legend.scls.User">
        <constructor-arg index="0" type="int" value="222"></constructor-arg>
        <constructor-arg index="1" type="java.lang.String" value="legend"></constructor-arg>
        <constructor-arg index="2" type="int" value="32"></constructor-arg>
    </bean>


</beans>